{
     "rules": {
         "indent": [2, 4],
         "quotes": [2, "single"],
         "linebreak-style": [2, "unix"],
         "semi": [2, "always"],
         "block-scoped-var": 2,
         "consistent-return": 1,
         "curly": 2,
         "eqeqeq": 2,
         "no-extra-bind": 2,
         "no-invalid-this": 2,
         "strict": [2, "global"],
         "no-catch-shadow": 2,
         "no-shadow": 2,
         "array-bracket-spacing": [2, "never"],
         "block-spacing": [2, "always"],
         "brace-style": 2,
         "comma-spacing": [2, {"before": false, "after": true}],
         "comma-style": [2, "last"],
         "computed-property-spacing": [2, "never"],
         "consistent-this": [2, "self"],
         "key-spacing": 2,
         "new-cap": 2,
         "new-parens": 2,
         "no-multiple-empty-lines": 2,
         "no-spaced-func": 2,
         "no-underscore-dangle": 2,
         "no-unneeded-ternary": 2,
         "quote-props": [2, "as-needed"],
         "no-console": 0
     },
     "env": {
         "es6": true,
         "node": true,
         "mocha": true
     },
     "extends": "eslint:recommended"
 }
